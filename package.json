{
  "name": "zero-nuxt3",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "prepare": "npx simple-git-hooks",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.16.1",
    "@iconify-json/mdi": "^1.1.66",
    "@iconify-json/svg-spinners": "^1.1.2",
    "@nuxt/eslint": "^0.3.10",
    "@nuxtjs/color-mode": "^3.4.1",
    "@pinia/nuxt": "^0.5.1",
    "@unocss/eslint-config": "^0.59.4",
    "@unocss/eslint-plugin": "^0.59.4",
    "@unocss/nuxt": "^0.59.4",
    "@vueuse/nuxt": "^10.9.0",
    "eslint": "^9.2.0",
    "eslint-plugin-format": "^0.1.1",
    "lint-staged": "^15.2.2",
    "npm-run-all": "^4.1.5",
    "nuxt": "^3.11.2",
    "pixi.js": "^8.1.1",
    "sass": "^1.76.0",
    "simple-git-hooks": "^2.11.1",
    "unocss": "^0.59.4",
    "vue": "^3.4.26",
    "vue-router": "^4.3.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowAny": [
        "*"
      ],
      "ignoreMissing": [
        "*"
      ]
    }
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*": [
      "eslint --fix"
    ]
  }
}
